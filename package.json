{
  "name": "line-chart",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "test": "jest",
    "build": "parcel build index.html --log-level 4",
    "lint": "eslint",
    "prettier:check": "prettier -c src/* test/* --single-quote",
    "prettier:write": "prettier -w src/* test/* --single-quote"
  },
  "dependencies": {
    "d3": "5.16.0",
    "topojson-client": "^3.1.0"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console": 2,
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 120,
          "tabWidth": 2
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.26.13",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "parcel-bundler": "^1.6.1",
    "prettier": "^2.1.2"
  },
  "keywords": [],
  "browserslist": [
    "last 1 Chrome version"
  ],
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "./test/utils/localStorageMock.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettier:check"
    }
  }
}
